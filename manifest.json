{
  "manifest_version": 3,
  "name": "T search",
  "version": "1.1",
  "description": "A Custom theme add browser and custom search engine add option with user and user see date and time and website URL add option",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_icon": "img/icon/icon.png"
  },
  "icons": {
    "128": "img/icon/icon.png",
    "48": "img/icon/icon32.png",
    "16": "img/icon/icon16.png"
  },
  "permissions": [
    "history",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "options_page": "options.html" 
}
